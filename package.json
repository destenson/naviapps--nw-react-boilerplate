{
  "name": "nw-react-boilerplate",
  "version": "0.0.3",
  "description": "NW.js + React.js Boilerplate",
  "keywords": [
    "nw",
    "nw.js",
    "nwjs",
    "node-webkit",
    "react",
    "boilerplate"
  ],
  "bugs": {
    "url": "https://github.com/naviapps/nw-react-boilerplate/issues"
  },
  "license": "MIT",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/naviapps/nw-react-boilerplate.git"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack.config.prod.js; build --tasks win-32,win-x64,mac-x64 .",
    "lint": "eslint src; exit 0"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-saga": "^0.15.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.47.0",
    "nwjs-builder-phoenix": "^1.14.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "build": {
    "nwVersion": "0.22.3",
    "files": [
      "./index.html",
      "./dist/bundle.js"
    ]
  }
